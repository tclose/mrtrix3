tckmap tckmap/in.tck -template dwi.mif - | testing_diff_data - tckmap/tdi.mif 2
tckmap tckmap/in.tck -vox 1 - | testing_diff_data - tckmap/tdi_vox1.mif 2
tckmap tckmap/in.tck -template dwi.mif -dec - | testing_diff_data - tckmap/tdi_color.mif 2
tckmap tckmap/in.tck -tod 6 -template dwi.mif - | testing_diff_data - tckmap/tod_lmax6.mif 2
